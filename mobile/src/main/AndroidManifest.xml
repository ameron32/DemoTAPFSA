<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.ameron32.tap.fsa.demotapfsa">

    <uses-permission android:name="android.permission.WAKE_LOCK" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainMenuActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".alpha.MainActivity" />
        <activity
            android:name=".alpha.territory.TerritoryActivity"
            android:label="@string/title_activity_territory"
            android:parentActivityName=".alpha.MainActivity"
            android:theme="@style/AppTheme.NoActionBar">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.ameron32.tap.fsa.demotapfsa.alpha.MainActivity" />
        </activity>
        <activity
            android:name=".alpha.callbook.CallbookActivity"
            android:label="@string/title_activity_callbook"
            android:parentActivityName=".alpha.MainActivity"
            android:theme="@style/AppTheme.NoActionBar">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.ameron32.tap.fsa.demotapfsa.alpha.MainActivity" />
        </activity>
        <activity
            android:name=".alpha.notathomes.NotAtHomesActivity"
            android:label="@string/title_activity_not_at_homes"
            android:parentActivityName=".alpha.MainActivity"
            android:theme="@style/AppTheme.NoActionBar">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.ameron32.tap.fsa.demotapfsa.alpha.MainActivity" />
        </activity>
        <activity
            android:name=".alpha.reports.ReportsActivity"
            android:label="@string/title_activity_reports"
            android:parentActivityName=".alpha.MainActivity"
            android:theme="@style/AppTheme.NoActionBar">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.ameron32.tap.fsa.demotapfsa.alpha.MainActivity" />
        </activity>
    </application>

</manifest>
